colorscheme Tomorrow-Night
set showtabline=never
set hintkeys='asdfghjkl'
hi -a Hint font-size: 11pt !important;
hi CmdLine -append font-size:12px
hi CompDesc -append font-size:12px
hi FontCode font-family: Fira Code Retina !important;

set hlfind
set incfind
set findcase=smart
js editor.defaultRegister="+"
nmap j 5<Down>
nmap k 5<Up>
nmap J :tabprevious<CR>
nmap K :tabnext<CR>
